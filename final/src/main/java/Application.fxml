<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.Controller">
   <children>
      <Button fx:id="logoutButton" layoutX="205.0" layoutY="60.0" mnemonicParsing="false" onAction="#switchToLoginScreen" prefHeight="25.0" prefWidth="80.0" style="-fx-background-color: null; -fx-border-color: black; -fx-border-width: 2;" text="Logout" />
      <TextArea fx:id="showAttachmentsList" editable="false" layoutX="88.0" layoutY="100.0" prefHeight="240.0" prefWidth="440.0" style="-fx-border-color: #8fffad; -fx-border-width: 2;" wrapText="true" />
      <Button fx:id="processButton" layoutX="331.0" layoutY="60.0" mnemonicParsing="false" onAction="#readEmailContent" prefHeight="25.0" prefWidth="80.0" style="-fx-background-color: null; -fx-border-color: black; -fx-border-width: 2;" text="Process" />
      <Button fx:id="deleteAttachButton" layoutX="245.0" layoutY="399.0" mnemonicParsing="false" onAction="#deleteFiles" prefHeight="29.0" prefWidth="126.0" style="-fx-background-color: null; -fx-border-color: black; -fx-border-width: 2;" text="Delete Attachments" />
      <Button fx:id="openAttachButton" layoutX="245.0" layoutY="353.0" mnemonicParsing="false" onAction="#openAttaches" prefHeight="29.0" prefWidth="126.0" style="-fx-background-color: null; -fx-border-color: black; -fx-border-width: 2;" text="Open Attachments" />
      <Label layoutY="-5.0" prefHeight="38.0" prefWidth="600.0" style="-fx-background-color: #ffb8b3;" />
      <Button fx:id="closeButton" layoutX="550.0" mnemonicParsing="false" onAction="#closeButton" prefHeight="10.0" prefWidth="50.0" style="-fx-border-color: black; -fx-background-color: #00f5c0; -fx-border-width: 1;" text="X">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="minimizeButton" alignment="CENTER" layoutX="500.0" mnemonicParsing="false" onAction="#minimize" prefHeight="10.0" prefWidth="50.0" style="-fx-border-color: black; -fx-background-color: #00f5c0; -fx-border-width: 1;" text="-">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <font>
            <Font size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
