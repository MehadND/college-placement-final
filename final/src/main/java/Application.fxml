<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.Controller">
   <children>
      <Button fx:id="logoutButton" layoutX="205.0" layoutY="60.0" mnemonicParsing="false" onAction="#switchToScene1" prefHeight="25.0" prefWidth="80.0" style="-fx-background-color: null; -fx-border-color: black; -fx-border-width: 2;" text="Logout" />
      <TextArea fx:id="showAttachmentsList" editable="false" layoutX="88.0" layoutY="100.0" prefHeight="225.0" prefWidth="424.0" style="-fx-border-color: #8fffad; -fx-border-width: 2;" wrapText="true" />
      <Button fx:id="processButton" layoutX="356.0" layoutY="60.0" mnemonicParsing="false" onAction="#downloadAttachments" prefHeight="25.0" prefWidth="80.0" style="-fx-background-color: null; -fx-border-color: black; -fx-border-width: 2;" text="Process" />
      <Button fx:id="deleteAttachButton" layoutX="245.0" layoutY="399.0" mnemonicParsing="false" onAction="#deleteFiles" prefHeight="25.0" prefWidth="111.0" style="-fx-background-color: null; -fx-border-color: black; -fx-border-width: 2;" text="Delete Attachments" />
      <Button fx:id="openAttachButton" layoutX="245.0" layoutY="354.0" mnemonicParsing="false" onAction="#openAttaches" style="-fx-background-color: null; -fx-border-color: black; -fx-border-width: 2;" text="Open Attachment" />
   </children>
</AnchorPane>
